{
  "compilerOptions": {
    "incremental": true,
    "target": "es2016",
    "experimentalDecorators": true,
    "module": "commonjs",
    "baseUrl": "./",
    "resolveJsonModule": true,
    "outDir": "dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
